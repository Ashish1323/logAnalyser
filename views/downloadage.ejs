<body>
    <h1 style="text-align:center; padding-top:10px;">Log Data</h1>
    <div class="container align-items-center text-center" style="margin-top: 51px;margin-bottom: 51px;"><table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>USER-NAME</th>
                <th>IP-ADDRESS</th>
                <th>REQUEST</th>
                <th>API-DEFINITION</th>
                <th>INFO</th>
                <th>TIME STAMP</th>
            </tr>
        </thead>
        <tbody>
            <% result.forEach(function(data){ %>
                <tr>
                    <td><%=data.USERNAME %></td>
                    <td><%=data.IP %></td>
                    <td><%=data.REQUEST %></td>
                    <td><%=data.API %></td>
                    <td><%=data.INFO %></td>
                    <td><%=data.TimeStamp %></td>
                </tr>
           <% }) %>
            
        </tbody>
    </table>
    <a class="btn btn-primary btn-success m-5" href="/download">download</a>
    <!-- <button class="btn btn-primary btn-success m-5" type="button" href="/download">Download CSV File</button> -->
</div>
